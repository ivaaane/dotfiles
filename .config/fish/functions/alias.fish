abbr grep 'grep --color=auto'
abbr xclip 'xclip -selection clipboard'
abbr ls 'eza -T --icons --group-directories-first -I ".git" --git-ignore -L 2'
abbr clear 'printf "\033[3J\033[H\033[2J"'

abbr firefox 'firefox & disown'
abbr discord 'vesktop & disown'
abbr yt 'ytfzf'
abbr search 'ddgr'
abbr music 'kew'
abbr woman 'man'

abbr .. 'cd ..'
abbr ... 'cd ../..'
